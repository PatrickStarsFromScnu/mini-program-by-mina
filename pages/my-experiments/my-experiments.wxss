page {
  height: 100%;
}
.my-experiments-top {
  z-index: 999;
  position: sticky;
  top: 0;
  background-color: #fff;
  height: 110rpx;
  border-bottom: 1rpx solid rgb(235, 233, 233);
  font-size: 34rpx;
  margin-bottom: 24rpx;
}
.my-experiments-top-center {
  padding-top: 20rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
}
.line {
  height: 50%;
  border-right: 1rpx solid rgb(202, 200, 200)
}
.light {
  width: 24%;
  border: 4rpx solid #73CC0B
}
.light-left {
  position: absolute;
  top: 106rpx;
  left: 12%;
}
.light-right {
  position: absolute;
  top: 106rpx;
  right: 12%;
}
.half-col {
  width: 50%;
  height: 90rpx;
  line-height: 80rpx;
  text-align: center;
}

.my-scroll-text {
  font-size: 32rpx;
  margin-left: 30rpx;
  margin-bottom: 20rpx;
}
.my-big-button {
  width: 90%;
  height: 80rpx;
  font-size: 34rpx;
  color: white;
  background-color: #EA7382;
  margin-bottom: 30rpx;
}
.my-small-button {
  width: 200rpx;
  height: 50rpx;
  font-size: 22rpx;
  color: white;
  margin: 0 40rpx;
  background-color: #EA7382;
}
.small-button-around {
  height: 100rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.no-experiments {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 300rpx;
  font-size: 30rpx;
  color: rgb(148, 147, 147)
}
